<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TodolistServiceService WSDL service -->

  <service name="TodolistService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://experiment8.DivyaPutta.visualstudio.com"/>
      <parameter name="wsdlServiceElement" value="TodolistServiceService"/>
      <parameter name="schemaQualified" value="http://experiment8.DivyaPutta.visualstudio.com"/>
      <parameter name="wsdlServicePort" value="TodolistService"/>
      <parameter name="className" value="com.visualstudio.DivyaPutta.experiment8.TodolistServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TodolistService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="todolist" qname="operNS:todolist" xmlns:operNS="http://experiment8.DivyaPutta.visualstudio.com" returnQName="retNS:todolistReturn" xmlns:retNS="http://experiment8.DivyaPutta.visualstudio.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:input_your_list_items" xmlns:pns="http://experiment8.DivyaPutta.visualstudio.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="todolist"/>

  </service>
</deployment>
