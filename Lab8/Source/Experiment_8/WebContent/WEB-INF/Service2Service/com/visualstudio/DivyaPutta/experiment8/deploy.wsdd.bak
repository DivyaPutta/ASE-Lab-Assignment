<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from Service2Service WSDL service -->

  <service name="Service2" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://experiment8.DivyaPutta.visualstudio.com"/>
      <parameter name="wsdlServiceElement" value="Service2Service"/>
      <parameter name="schemaQualified" value="http://experiment8.DivyaPutta.visualstudio.com"/>
      <parameter name="wsdlServicePort" value="Service2"/>
      <parameter name="className" value="com.visualstudio.DivyaPutta.experiment8.Service2SoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Service2"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="helloName" qname="operNS:helloName" xmlns:operNS="http://experiment8.DivyaPutta.visualstudio.com" returnQName="retNS:helloNameReturn" xmlns:retNS="http://experiment8.DivyaPutta.visualstudio.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:Enter_your_name" xmlns:pns="http://experiment8.DivyaPutta.visualstudio.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="helloName"/>

  </service>
</deployment>
